      <!-- Include header -->
      <%- include("partials/header.ejs") %>
      <!-- /Include header -->

      <div class="page-wrapper">
        <div class="container-fluid">
          <div class="row">

        <!-- Include aside -->
        <%- include("partials/aside.ejs") %>
        <!-- /Include aside -->

          <div class="col-xl-9 col-lg-8 col-md-12">
            <div class="card shadow-sm grow ctm-border-radius">
              <div class="card-body align-center">
                <h4 class="card-title float-left mb-0 mt-2"><span id="clients-count"></span> Clients</h4>
                <ul class="nav nav-tabs float-right border-0 tab-list-emp">
                  <li class="nav-item pl-3">
                    <a class="btn btn-theme button-1 text-white ctm-border-radius p-2 add-person ctm-btn-padding" id="add-client">
                      <i class="fa fa-plus"></i> Add Client</a>
                    <a class="btn btn-theme button-1 text-white ctm-border-radius p-2 add-person ctm-btn-padding" id="all-client">
                      <i class="fa fa-users"></i> All Client</a>
                  </li>
                </ul>
              </div>
            </div>
            <div class="card shadow-sm grow ctm-border-radius" id="pagination">
              <div class="card-body d-flex justify-content-between">
                <a class="btn btn-theme button-1 text-white ctm-border-radius add-person ctm-btn-padding" id="previous">
                  <i class="fa fa-angles-left"></i>&nbsp;Previous
                </a>
                <a class="btn btn-theme button-1 text-white ctm-border-radius add-person ctm-btn-padding" style="pointer-events:none" id="current"></a>
                <a class="btn btn-theme button-1 text-white ctm-border-radius add-person ctm-btn-padding" id="next" >
                  Next&nbsp;<i class="fa fa-angles-right"></i>
                </a>
              </div>
            </div>
            <div class="ctm-border-radius shadow-sm grow card" id="clients-data-body">
              <div class="card-body">
                <div class="row">
                  <div class="col-md-2 form-group">
                    <p class="pl-1 error">Sorting By:</p>
                    <select class="form-control" id="sort-by" onchange="sortByField()">
                      <option value="name">Name</option>
                      <option value="email">Email</option>
                      <option value="slackId">SlackId</option>
                      <option value="city">City</option>
                      <option value="state">State</option>
                      <option value="country">Country</option>
                      <option value="organization">Organization</option>
                    </select>
                  </div>
                  <div class="col-md-2 form-group">
                    <p class="pl-1 error">Sorting Order:</p>
                    <select class="form-control" id="sort-order" onchange="sortByField()">
                      <option value="ASC" selected>Ascending</option>
                      <option value="DESC">Descending</option>
                    </select>
                  </div>
                  <div class="col-md-4 form-group form-outline">
                    <p class="pl-1 error">Search:</p>
                    <input type="search" id="search-by" class="form-control" aria-label="Search" />
                  </div>
                  <div class="col-md-4 form-group">
                    <p class="pl-1 error">Number of records per page:</p>
                    <select class="form-control" id="select-record-count" onchange="showThisMuchRecord()">
                      <option value="6">6</option>
                      <option value="12" selected>12</option>
                      <option value="24">24</option>
                      <option value="51">51</option>
                    </select>
                  </div>
                </div>
                <div class="row people-grid-row" id="clients-data">
                  
                </div>
              </div>
            </div>
            <div class="ctm-border-radius shadow-sm grow card" id="clients-add-div">
              <div class="card-body">
                <div class="people-grid-row">
                  <form id="client-register-form">
                    <div class="mb-3">
                      <label class="form-label text-orange lead">Name :</label>
                      <input type="text" class="form-control" name="name">
                    </div>
                    <div class="mb-3">
                      <label class="form-label text-orange lead">Email :</label>
                      <input type="email" class="form-control" name="email">
                    </div>
                    <div class="mb-3">
                      <label class="form-label text-orange lead">SlackId :</label>
                      <input type="text" class="form-control" name="slackId">
                    </div>
                    <div class="mb-3">
                      <label class="form-label text-orange lead">City :</label>
                      <input type="text" class="form-control" name="city">
                    </div>
                    <div class="mb-3">
                      <label class="form-label text-orange lead">State :</label>
                      <input type="text" class="form-control" name="state">
                    </div>
                    <div class="mb-3">
                      <label class="form-label text-orange lead">Country :</label>
                      <input type="text" class="form-control" name="country">
                    </div>
                    <div class="mb-3">
                      <label class="form-label text-orange lead">Organization :</label>
                      <input type="text" class="form-control" name="organization">
                    </div>
                    <button type="submit" class="btn btn-theme button-1 text-white ctm-border-radius p-2 add-person ctm-btn-padding px-3"><h4>Register</h4></button>
                  </form>
                </div>
              </div>
            </div>

            <div class="ctm-border-radius shadow-sm grow card" id="clients-view-div">
              <div class="card-body">
                <div class="people-grid-row">
                  <form id="client-edit-form">
                    <div class="mb-3">
                      <label class="form-label text-orange lead">Name :</label>
                      <input type="text" class="form-control" name="name" id="client-edit-name" value="${resData.data.name}">
                    </div>
                    <div class="mb-3">
                      <label class="form-label text-orange lead">City :</label>
                      <input type="text" class="form-control" name="city" id="client-edit-city" value="${resData.data.city}">
                    </div>
                    <div class="mb-3">
                      <label class="form-label text-orange lead">State :</label>
                      <input type="text" class="form-control" name="state" id="client-edit-state" value="${resData.data.state}">
                    </div>
                    <div class="mb-3">
                      <label class="form-label text-orange lead">Country :</label>
                      <input type="text" class="form-control" name="country" id="client-edit-country" value="${resData.data.country}">
                    </div>
                    <div class="mb-3">
                      <label class="form-label text-orange lead">Organization :</label>
                      <input type="text" class="form-control" name="organization" id="client-edit-organization">
                    </div>
                    <div class="d-flex justify-content-between">
                      <button type="submit" class="btn btn-theme button-1 text-white ctm-border-radius p-2 add-person ctm-btn-padding px-3" id="client-edit-submit"><h4>Update</h4></button>
                      <p class="btn btn-theme button-1 text-white ctm-border-radius p-2 add-person ctm-btn-padding px-3" id="disable-client" onclick="disableClient()">Disable</p>
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="sidebar-overlay" id="sidebar_overlay"></div>
  <script src="/public/js/jquery-3.2.1.min.js"></script>

  <script src="/public/js/popper.min.js"></script>
  <script src="/public/js/bootstrap.min.js"></script>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.3/jquery.validate.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/jquery-validation@1.19.3/dist/additional-methods.min.js"></script>

  <script src="/public/plugins/theia-sticky-sidebar/ResizeSensor.js"></script>
  <script src="/public/plugins/theia-sticky-sidebar/theia-sticky-sidebar.js"></script>

  <script src="/public/plugins/select2/moment.min.js"></script>
  <script src="/public/js/bootstrap-datetimepicker.min.js"></script>

  <script src="/public/plugins/select2/select2.min.js"></script>

  <script src="/public/plugins/bootstrap-tagsinput/bootstrap-tagsinput.min.js"></script>
    
  <script src="/public/js/script.js"></script>
	<script src="/public/js/client.js"></script>
  
</body>
</html>